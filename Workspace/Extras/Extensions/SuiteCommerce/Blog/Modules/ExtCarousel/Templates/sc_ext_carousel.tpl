<section aria-labelledby="carousel-title">
<div class="sc-ext-carousel" data-carousel-id="{{carouselId}}">
  <h5 class="sc-ext-carousel-title" id="carousel-title">{{carouselTitle}}</h5>
  <ul class="sc-ext-carousel-slides transition-{{transition}}" data-view="Slides.View" aria-labelledby="carousel-title"></ul>
  {{#if showIndicators}}
    <div class="sc-ext-carousel-indicators" data-view="Indicators.View">
    </div>
  {{/if}}
</div>
</section>
