<main>
<div class="sc-blog-post-default-layout">
  <div class="sc-blog-post-default-layout-container">
    <div class="sc-blog-post-default-layout-csm-area-full-width-column"
         data-cms-area="all-blog-post-pages-cms-area-1" data-cms-area-filters="page_type">
    </div>
    <div class="sc-blog-post-default-layout-csm-area-full-width-column"
         data-cms-area="blog-post-cms-area-1" data-cms-area-filters="path">
    </div>
    <div class="sc-blog-post-default-layout-csm-area-row">
      <div class="sc-blog-post-default-layout-csm-area-column" data-cms-area="blog-post-cms-area-2"
           data-cms-area-filters="path">
      </div>
      <div class="sc-blog-post-default-layout-csm-area-column" data-cms-area="blog-post-cms-area-3"
           data-cms-area-filters="path">
      </div>
    </div>
    <div class="sc-blog-post-default-layout-clear-box"></div>
  </div>

  <div class="sc-blog-post-default-layout-container" data-component="header">
    <div class="sc-blog-post-default-layout-content-box-full-width">
      <div class="sc-blog-post-default-layout-content-box-partial-medium" data-view="Header.View"></div>
      <div class="sc-blog-post-default-layout-clear-box"></div>
    </div>
    <div class="sc-blog-post-default-layout-clear-box"></div>
  </div>

  <div class="sc-blog-post-default-layout-row" data-component="header-image" data-view="Header.Image.View"></div>

  <div class="sc-blog-post-default-layout-container">
    <div class="sc-blog-post-default-layout-csm-area-full-width-column"
         data-cms-area="all-blog-post-pages-cms-area-2" data-cms-area-filters="page_type">
    </div>
    <div class="sc-blog-post-default-layout-csm-area-row">
      <div class="sc-blog-post-default-layout-csm-area-column" data-cms-area="blog-post-cms-area-4"
           data-cms-area-filters="path">
      </div>
      <div class="sc-blog-post-default-layout-csm-area-column" data-cms-area="blog-post-cms-area-5"
           data-cms-area-filters="path">
      </div>
      <div class="sc-blog-post-default-layout-clear-box"></div>
    </div>
    <div class="sc-blog-post-default-layout-csm-area-full-width-column"
         data-cms-area="blog-post-cms-area-6" data-cms-area-filters="path">
    </div>
    <div class="sc-blog-post-default-layout-clear-box"></div>
  </div>

  <div class="sc-blog-post-default-layout-container" data-component="content">
    <div class="sc-blog-post-default-layout-content-box-full-width">
      <div class="sc-blog-post-default-layout-content-box-partial-medium" data-view="Content.View"></div>
      <div class="sc-blog-post-default-layout-clear-box"></div>
    </div>
    <div class="sc-blog-post-default-layout-clear-box"></div>
  </div>

  <div class="sc-blog-post-default-layout-container">
    <div class="sc-blog-post-default-layout-csm-area-full-width-column"
         data-cms-area="blog-post-cms-area-7" data-cms-area-filters="path">
    </div>
    <div class="sc-blog-post-default-layout-csm-area-full-width-column"
         data-cms-area="all-blog-post-pages-cms-area-3" data-cms-area-filters="page_type">
    </div>
    <div class="sc-blog-post-default-layout-column-container">
      <div class="sc-blog-post-default-layout-csm-area-column"
           data-cms-area="all-blog-post-pages-cms-area-4" data-cms-area-filters="page_type">
      </div>
      <div class="sc-blog-post-default-layout-csm-area-column"
           data-cms-area="all-blog-post-pages-cms-area-5" data-cms-area-filters="page_type">
      </div>
      <div class="sc-blog-post-default-layout-clear-box"></div>
    </div>
    <div class="sc-blog-post-default-layout-clear-box"></div>
  </div>

  <div class="sc-blog-post-default-layout-container" data-component="footer">
    <div class="sc-blog-post-default-layout-content-box-full-width">
      <div class="sc-blog-post-default-layout-content-box-partial-medium" data-view="Footer.View"></div>
      <div class="sc-blog-post-default-layout-clear-box"></div>
    </div>
  </div>
</div>
</main>
