<div class="sc-blog-post-taxonomy-navigation" role="navigation" aria-labelledby="section-title" tabindex="0">
  <h5 class="sc-blog-post-taxonomy-navigation-list-title" data-action="expand-taxonomy-list">
    <span class="sc-blog-post-taxonomy-navigation-list-title-large-devices" id="section-title">
      {{sectionTitle}}
    </span>
    <span class="sc-blog-post-taxonomy-navigation-list-title-small-devices">
      {{#if activeTaxonomy}}{{activeTaxonomy}}{{else}}{{sectionTitle}}{{/if}}
    </span>
    <span class="sc-blog-post-taxonomy-navigation-list-title-icon"></span>
  </h5>
  <div class="sc-blog-post-taxonomy-navigation-container">
    <ul data-view="Taxonomies.View"></ul>
  </div>
</div>
